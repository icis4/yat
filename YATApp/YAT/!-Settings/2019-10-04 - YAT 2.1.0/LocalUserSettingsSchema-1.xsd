    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="1" name="Enabled" type="xs:boolean" />
      <xs:element minOccurs="1" maxOccurs="1" name="Interval" type="xs:int" />
    </xs:sequence>
  </xs:complexType>
