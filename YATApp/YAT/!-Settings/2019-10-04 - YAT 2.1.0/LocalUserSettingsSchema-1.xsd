    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="1" name="SeparateRxId" type="xs:boolean" />
      <xs:element minOccurs="1" maxOccurs="1" name="AnyRxId" type="xs:boolean" />
      <xs:element minOccurs="1" maxOccurs="1" name="RxId" type="xs:unsignedByte" />
