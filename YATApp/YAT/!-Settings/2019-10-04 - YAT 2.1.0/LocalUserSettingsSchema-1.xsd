      <xs:enumeration value="ZeroTerminated" />
      <xs:enumeration value="MT_SerHid" />
      <xs:enumeration value="TI_HidApi" />
      <xs:enumeration value="Signal11_HidApi" />
      <xs:enumeration value="YAT" />
    </xs:restriction>
  </xs:simpleType>
